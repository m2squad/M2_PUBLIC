    print('â››  M2  â››')
    print('â  á´á´€á´…Éªá´œÉ´ á´Éªá´…É´ÉªÉ¢Êœá´› ÉªÉ´Ò“ÉªÉ´Éªá´›Ê')
    print('â  á´¡á´‡ á´€Ê€á´‡ á´œÉ´Éªá´›Ê .. á´¡á´‡ á´€Ê€á´‡ ÉªÉ´Ò“ÉªÉ´Éªá´›Ê')
    print('â•”â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•—')
    print('                  M2 SQUAD')
    print('â•šâ•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•')
    print('â  sá´„Ê€Éªá´˜á´› Ê™Ê â€¢ ÊŸá´‡á´›á´á´‡_sÊŸá´‡á´‡á´˜')
    print('â  á´„á´á´˜ÊÊ€ÉªÉ¢Êœá´›s Â© 2020, M2. á´€ÊŸÊŸ Ê€ÉªÉ¢Êœá´› Ê€á´‡sá´‡Ê€á´ á´‡á´…')
