    "✧═════• ᴇxɪᴛ •═════✧"
  }, nil, (os.date("━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━\n❏ M2 ❏\n➠  sᴄʀɪᴘᴛ ʙʏ・ʟᴇᴛ ᴍᴇ sʟᴇᴇᴘ\n━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━\n%A, %d %B %Y %H:%M%p")))
  if WPN1 == nil then
  else
    if WPN1[1] == true then M2SQUAD() end
    if WPN1[2] == true then M2SAFE() end
    if WPN1[3] == true then M2WALLHACK() end
    if WPN1[4] == true then M2BRT() end
    if WPN1[5] == true then M2CLOSE() end
  end
  PUBGMH = -1
end

