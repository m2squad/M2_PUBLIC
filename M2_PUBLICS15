end

function M2SQUAD()
  bypassAlert =
  gg.alert ("â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”\nâ M2 â\nâ   sá´„Ê€Éªá´˜á´› Ê™Êãƒ»ÊŸá´‡á´› á´á´‡ sÊŸá´‡á´‡á´˜\nâ”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”\n                 âš ï¸ Run in Lobby âš ï¸\nâ”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”",
  "GLOBAL","KOREAï¸","exit")
   if bypassAlert == nil then
  else
  if bypassAlert == 1 then M2BPASS1() end
  if bypassAlert == 2 then M2BPASS1() end
end
end
function M2WALLHACK()
  WALL11 = 
  gg.alert ("â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”\nâ M2 â\nâ   sá´„Ê€Éªá´˜á´› Ê™Êãƒ»ÊŸá´‡á´› á´á´‡ sÊŸá´‡á´‡á´˜\nâ”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”\n       âš ï¸ á´É´ÊŸÊ Ò“á´Ê€ sÉ´á´€á´˜á´…Ê€á´€É¢á´É´ 712-845 âš ï¸\nâ”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”",
  "green", "Yellow mix", "other device") 
 if WALL11 == nil then
  else
  if WALL11 == 1 then CL41() end
  if WALL11 == 2 then CL43() end
  if WALL11 == 3 then M2WHALL() end
end
end

function M2WHALL()
M2 = gg.makeRequest("https://pastebin.com/raw/1q1GzbCU").content
pcall(load(M2))
end


while true do
  if gg.isVisible(true) then
    PUBGMH = 1
    gg.setVisible(false)
  end
  gg.clearResults()
  if PUBGMH == 1 then
    M2HOME()
  end
end
