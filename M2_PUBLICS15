function M2CLOSE()
    print('⛛  M2  ⛛')
    print('➠ ᴍᴀᴅɪᴜɴ ᴍɪᴅɴɪɢʜᴛ ɪɴғɪɴɪᴛʏ')
    print('➠ ᴡᴇ ᴀʀᴇ ᴜɴɪᴛʏ .. ᴡᴇ ᴀʀᴇ ɪɴғɪɴɪᴛʏ')
    print('╔═════════════════╗')
    print('                  M2 SQUAD')
    print('╚═════════════════╝')
    print('➠ sᴄʀɪᴘᴛ ʙʏ • ʟᴇᴛᴍᴇ_sʟᴇᴇᴘ')
    print('➠ ᴄᴏᴘʏʀɪɢʜᴛs © 2020, M2. ᴀʟʟ ʀɪɢʜᴛ ʀᴇsᴇʀᴠᴇᴅ')
    gg.skipRestoreState()
    gg.setVisible(true)
    os.exit()
end

function M2SQUAD()
  bypassAlert =
  gg.alert ("━━━━━━━━━━━━━━━━━━━━━━━━━━━━\n❏ M2 ❏\n➠  sᴄʀɪᴘᴛ ʙʏ・ʟᴇᴛ ᴍᴇ sʟᴇᴇᴘ\n━━━━━━━━━━━━━━━━━━━━━━━━━━━━\n                 ⚠️ Run in Lobby ⚠️\n━━━━━━━━━━━━━━━━━━━━━━━━━━━━",
  "GLOBAL","KOREA️","exit")
   if bypassAlert == nil then
  else
  if bypassAlert == 1 then M2BPASS1() end
  if bypassAlert == 2 then M2BPASS1() end
end
end
