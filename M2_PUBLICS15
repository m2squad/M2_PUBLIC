function M2SAFE()
  gg.setVisible(false)
  gg.clearResults()
  gg.setRanges(32)
  gg.searchNumber("1,868,784,978;1,850,305,641;28,518;13,212::13", 4, false, 536870912, 0, -1)
  gg.searchNumber("1,850,305,641", 4, false, 536870912, 0, -1)
