gg.alert ("━━━━━━━━━━━━━━━━━━━━━━━━━━━━\n      ⚠️    ᴡɪᴛʜ ᴜʀ ᴏᴡɴ ʀɪsᴋ    ⚠️\n━━━━━━━━━━━━━━━━━━━━━━━━━━━━")
    gg.clearResults()
gg.setRanges(gg.REGION_ANONYMOUS)
gg.searchNumber("25;23;30.5",gg.TYPE_FLOAT,false,gg.SIGN_EQUAL,0,-1)
gg.searchNumber("23;25;30.5",gg.TYPE_FLOAT,false,gg.SIGN_EQUAL,0,-1)
gg.searchNumber("25;23;30.5",gg.TYPE_FLOAT,false,gg.SIGN_EQUAL,0,-1)
gg.searchNumber("23;25;30.5",gg.TYPE_FLOAT,false,gg.SIGN_EQUAL,0,-1)
